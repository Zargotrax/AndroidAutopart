<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SupplierDetailActivity">

    <TextView
        android:id="@+id/txtView_adpc"
        android:layout_width="158dp"
        android:layout_height="21dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toEndOf="@+id/txtView_adpc_label"
        app:layout_constraintTop_toBottomOf="@+id/txtView_adpr" />

    <TextView
        android:id="@+id/txtView_telephone"
        android:layout_width="233dp"
        android:layout_height="19dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="19dp"
        app:layout_constraintStart_toEndOf="@+id/txtView_telephone_label"
        app:layout_constraintTop_toBottomOf="@+id/txtView_adpc" />

    <TextView
        android:id="@+id/txtView_contact"
        android:layout_width="197dp"
        android:layout_height="19dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toEndOf="@+id/txtView_contact_label"
        app:layout_constraintTop_toBottomOf="@+id/txtView_telephone" />

    <TextView
        android:id="@+id/txtView_adct"
        android:layout_width="208dp"
        android:layout_height="18dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="21dp"
        app:layout_constraintStart_toEndOf="@+id/txtView_adct_label"
        app:layout_constraintTop_toBottomOf="@+id/txtView_ad2" />

    <TextView
        android:id="@+id/txtView_adpr"
        android:layout_width="181dp"
        android:layout_height="19dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="21dp"
        app:layout_constraintStart_toEndOf="@+id/txtView_adpr_label"
        app:layout_constraintTop_toBottomOf="@+id/txtView_adct" />

    <TextView
        android:id="@+id/txtView_id_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="61dp"
        android:text="ID : "
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtView_name_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:text="Name :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtView_id_label" />

    <TextView
        android:id="@+id/txtView_ad1_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:text="Address Line 1 :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtView_name_label" />

    <TextView
        android:id="@+id/txtView_ad2_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:text="Address Line 2 :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtView_ad1_label" />

    <TextView
        android:id="@+id/txtView_adct_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:text="Address City :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtView_ad2_label" />

    <TextView
        android:id="@+id/txtView_adpr_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:text="Address Province :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtView_adct_label" />

    <TextView
        android:id="@+id/txtView_adpc_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:text="Address Postal Code :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtView_adpr_label" />

    <TextView
        android:id="@+id/txtView_telephone_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:text="Telephone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtView_adpc_label" />

    <TextView
        android:id="@+id/txtView_contact_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:text="Contact Name :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtView_telephone_label" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="163dp"
        android:layout_marginTop="114dp"
        android:text="Back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtView_adpc_label" />

    <TextView
        android:id="@+id/txtView_id"
        android:layout_width="274dp"
        android:layout_height="19dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toEndOf="@+id/txtView_id_label"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtView_name"
        android:layout_width="254dp"
        android:layout_height="18dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="21dp"
        app:layout_constraintStart_toEndOf="@+id/txtView_name_label"
        app:layout_constraintTop_toBottomOf="@+id/txtView_id" />

    <TextView
        android:id="@+id/txtView_ad1"
        android:layout_width="198dp"
        android:layout_height="18dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="21dp"
        app:layout_constraintStart_toEndOf="@+id/txtView_ad1_label"
        app:layout_constraintTop_toBottomOf="@+id/txtView_name" />

    <TextView
        android:id="@+id/txtView_ad2"
        android:layout_width="196dp"
        android:layout_height="18dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="22dp"
        app:layout_constraintStart_toEndOf="@+id/txtView_ad2_label"
        app:layout_constraintTop_toBottomOf="@+id/txtView_ad1" />
</androidx.constraintlayout.widget.ConstraintLayout>